{
  "name": "@usevoiceai/hume",
  "version": "0.1.2",
  "description": "Hume text-to-speech provider for the usevoiceai server pipeline.",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --clean --format esm --tsconfig tsconfig.build.json",
    "dev": "tsup src/index.ts --dts --watch --format esm --tsconfig tsconfig.build.json",
    "test": "vitest run",
    "lint": "tsc --noEmit -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@usevoiceai/server": "workspace:*"
  },
  "peerDependencies": {
    "hume": "^0.8.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankitiscracked/usevoiceai.git"
  },
  "bugs": {
    "url": "https://github.com/ankitiscracked/usevoiceai/issues"
  },
  "homepage": "https://github.com/ankitiscracked/usevoiceai#readme",
  "keywords": [
    "voice",
    "ai",
    "tts",
    "hume",
    "usevoiceai"
  ]
}
