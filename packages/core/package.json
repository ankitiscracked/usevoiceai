{
  "name": "@usevoiceai/core",
  "version": "0.1.2",
  "description": "Core voice session client with recorder, socket client, controller, and audio streaming utilities.",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --clean --format esm",
    "dev": "tsup src/index.ts --dts --watch --format esm",
    "test": "vitest run",
    "lint": "tsc --noEmit -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ankitiscracked/usevoiceai.git"
  },
  "bugs": {
    "url": "https://github.com/ankitiscracked/usevoiceai/issues"
  },
  "homepage": "https://github.com/ankitiscracked/usevoiceai#readme",
  "keywords": [
    "voice",
    "ai",
    "stt",
    "tts",
    "usevoiceai"
  ]
}
